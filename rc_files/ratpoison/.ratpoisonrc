definekey top F9  exec setxkbmap fr -option caps:escape
definekey top F10 exec setxkbmap us altgr-intl -option caps:escape

bind e exec emacs
bind f exec firefox
bind l exec suspend.sh
bind C-j exec ratpoison -c "select Navigator"
bind C-k exec ratpoison -c "select emacs"
bind Up exec xbacklight -inc 25
bind Down exec xbacklight -dec 25

set fwcolor grey
set winname name

exec wmname LG3D